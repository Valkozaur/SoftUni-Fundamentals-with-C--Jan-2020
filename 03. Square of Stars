using System;

    class Program
{
    static void Main(string[] args)
    {
        int numberOfLines = int.Parse(Console.ReadLine());

        for (int i = 0; i < numberOfLines; i++)
        {
            for (int k = 0; k < numberOfLines; k++)
            {
                Console.Write("* ");
            }
            Console.WriteLine();
        }
    }
}
