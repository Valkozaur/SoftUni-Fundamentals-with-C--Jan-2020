using System;
using System.Linq;
using System.Collections.Generic;

namespace Data_Types_and_Basic_Syntax___Exercise
{
    class Program
    {
        static void Main(string[] args)
        {
            //8.Beer Kegs

            int numberOfLines = int.Parse(Console.ReadLine());
            List<Keg> Kegs = new List<Keg>(numberOfLines);
                

            for (int i = 0; i < numberOfLines; i++)
            {
                string model = Console.ReadLine();
                double radius = double.Parse(Console.ReadLine());
                double height = double.Parse(Console.ReadLine());

                double currentKegVolume = Math.PI * Math.Pow(radius, 2) * height;

                Keg keg = new Keg(model, currentKegVolume);
                Kegs[i] = keg;
            }

            Console.WriteLine();

        }
    }

    class Keg
    {
        public Keg(string model, double volume)
        {
            Model = model;
            Volume = volume;
        }
        public static string Model { get; set; }
        public static double Volume { get; set; }
    }
}
